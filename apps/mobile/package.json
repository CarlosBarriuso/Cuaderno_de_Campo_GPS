{
  "name": "@cuaderno-campo/mobile",
  "version": "1.0.0",
  "description": "Aplicación móvil para Cuaderno de Campo GPS",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "detox test --configuration android.emu.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:build": "detox build --configuration android.emu.debug",
    "test:e2e:build:ios": "detox build --configuration ios.sim.debug",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.14.3",
    "@expo/vector-icons": "^14.0.4",
    "@nozbe/watermelondb": "^0.28.0",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.1.0",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/stack": "^7.1.1",
    "@tanstack/react-query": "^5.61.5",
    "expo": "~52.0.11",
    "expo-camera": "~16.0.8",
    "expo-constants": "~17.0.3",
    "expo-device": "~7.0.1",
    "expo-font": "~13.0.1",
    "expo-image-picker": "~16.0.3",
    "expo-location": "~18.0.4",
    "expo-router": "~4.0.9",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.55.0",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-maps": "1.18.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-svg": "15.8.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "ajv": "^8.17.1",
    "detox": "^20.40.1",
    "eslint": "^9.17.0",
    "eslint-config-expo": "^7.1.2",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.1",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.3.3"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "private": true
}
